{
  "package": "com.jsankangjb.akwellbeing",
  "name": "安康计步",
  "versionName": "1.0.0",
  "versionCode": 1,
  "minPlatformVersion": 1121,
  "icon": "/assets/images/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.fetch"
    },
    {
      "name": "system.app"
    },
    {
      "name": "system.storage"
    },
    {
      "name": "system.device"
    },
    {
      "name": "system.webview"
    },
    {
      "name": "system.request"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "template/official": "demo-template",
  "config": {
    "logLevel": "off",
    "designWidth": 250
  },
  "router": {
    "entry": "pages/Splash",
    "pages": {
      "pages/Splash": {
        "component": "index"
      },
      "pkg_other/pages/Web": {
        "component": "index"
      },
      "pkg_other/pages/BmiSetting": {
        "component": "index"
      },
      "pkg_main/pages/Main": {
        "component": "index"
      },
      "pkg_other/pages/Demo": {
        "component": "index"
      },
      "pkg_other/pages/DemoDetail": {
        "component": "index"
      }
    }
  },
  "subpackages": [
    {
      "name": "pkg_main",
      "resource": "pkg_main"
    },
    {
      "name": "pkg_other",
      "resource": "pkg_other"
    }
  ],
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "pages/Splash": {
        "titleBar": false,
        "menu": false
      },
      "pkg_other/pages/Web": {
        "titleBarText": "详情",
        "menu": false
      },
      "pkg_main/pages/Main": {
        "titleBar": false,
        "menu": false
      },
      "pkg_other/pages/BmiSetting": {
        "titleBar": false,
        "statusBar": {
          "immersive": true,
          "textStyle": "dark"
        }
      },
      "pkg_other/pages/Demo": {
        "titleBarText": "快应用示例模版",
        "menu": false
      },
      "pkg_other/pages/DemoDetail": {
        "titleBarText": "详情页"
      }
    }
  },
  "deviceTypeList": ["phone"]
}
